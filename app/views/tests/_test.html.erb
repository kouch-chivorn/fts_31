<div class=row>
  <div class="col-md-2"><%= link_to test.category.name, "#" %>
  </div>
  <div class="col-md-2"><%= test.created_at.strftime "%Y-%m-%d %H:%M"  %></div>
  <div class="col-md-2">
    <% if test.status == t(:start) || test.status == t(:testing)%>
      <%= link_to test.status, edit_test_path(test), id: "test_link"  %>
    <% else %>
      <%= link_to test.status, test_path(test), id: "test_link"  %>
    <% end %>
    <%= hidden_field_tag :reloadValue, "" %>
  </div>
</div>